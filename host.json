{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "enableLiveMetrics": false,
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    },
    "fileLoggingMode": "always",
    "logLevel": {
      "default": "Error",
      "Microsoft": "None",
      "Worker": "None",
      "Host": "Error",
      "Host.Aggregator": "Warning",
      "Function": "Error",
      "Function.orchestrator.User": "Debug",
      "Function.rate_limiter.User": "Debug",
      "Function.circuit_breaker.User": "Debug",
      "Function.meta_trigger.User": "Debug",
      "Function.meta_processor.User": "Debug",
      "Function.scraper_blob_trigger.User": "Debug",
      "Function.scraper_processor.User": "Debug",
      "Function.scraper_scheduled_trigger.User": "Debug",
      "Function.scraper_scheduled_processor.User": "Debug",
      "Function.parse_snap.User": "Debug",
      "Function.annotate_snap.User": "Debug",
      "Function.single_diff.User": "Debug",
      "Function.clean_diffs.User": "Debug",
      "Function.summarizer_blob_trigger.User": "Debug",
      "Function.summarizer_processor.User": "Debug",
      "Function.parse_summary.User": "Debug",
      "Function.claim_extractor_blob_trigger.User": "Debug",
      "Function.claim_extractor_processor.User": "Debug",
      "Function.parse_claims.User": "Debug",
      "Function.claim_checker_blob_trigger.User": "Debug",
      "Function.claim_checker_processor.User": "Debug",
      "Function.parse_factcheck.User": "Debug",
      "Function.judge_blob_trigger.User": "Debug",
      "Function.judge_processor.User": "Debug",
      "Function.parse_judge.User": "Debug",
      "src": "Debug"
    },
    "console": {
      "isEnabled": true
    }
  },
  "extensions": {
    "durableTask": {
      "hubName": "RateLimitedHub",
      "storageProvider": {
        "connectionStringName": "AzureWebJobsStorage"
      }
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[4.*, 5.0.0)"
  }
}